using System;

public class StringRepeater
{
    public static void Main()
    {
        var word = Console.ReadLine();
        int reapeat =int.Parse(Console.ReadLine());     
        var reapeatString = ReapeatString(reapeat, word);
        Console.WriteLine(reapeatString);
    }

    public static string ReapeatString(int reapeat,string word)
    {
        var result = "";
        for (int i = 0; i < reapeat; i++)
        {
            result = result + word;
        }

        return result;
    }
}

