using System;

public class Calculator
{
    public static void Main()
    {
        var numberOne =int.Parse(Console.ReadLine());
        string operation = Console.ReadLine();
        var numberTwo =int.Parse(Console.ReadLine());
        
        switch (operation)
        {
            case "*" :Console.WriteLine($"{numberOne} {operation} {numberTwo} = {numberOne * numberTwo}");
                break;
            case "+":
                Console.WriteLine($"{numberOne} {operation} {numberTwo} = {numberOne + numberTwo}");
                break;
            case "-":
                Console.WriteLine($"{numberOne} {operation} {numberTwo} = {numberOne - numberTwo}");
                break;
            case "/":
                Console.WriteLine($"{numberOne} {operation} {numberTwo} = {numberOne / numberTwo}");
                break;
        }
    }
}
