using System;

public class PracticeCharactersAndStrings
{
    public static void Main()
    {
        string text = "Software University";
        char letter1 = 'B';
        char letter2 = 'y';
        char letter3 = 'e';
        string text2 = "I love programming";

        Console.WriteLine(text);
        Console.WriteLine(letter1);
        Console.WriteLine(letter2);
        Console.WriteLine(letter3);
        Console.WriteLine(text2);
    }
}
