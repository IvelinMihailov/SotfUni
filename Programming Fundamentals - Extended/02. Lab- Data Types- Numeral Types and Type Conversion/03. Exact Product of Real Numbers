using System;

public class ExactProductOfRealNumbers
{
    public static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        decimal product = 1m;
        decimal numbers = 0m;

        for (int i = 0; i < n; i++)
        {
            numbers = decimal.Parse(Console.ReadLine());
            product = product * numbers;
        }

        Console.WriteLine(product);
    }
}
