using System;
using System.Linq;
using System.Numerics;

public class BigFactorial
{
    public static void Main()
    {
        int number = int.Parse(Console.ReadLine());

        BigInteger result = 1;

        for (int i = number; i > 1; i--)
        {
            result = result * i;
        }

        Console.WriteLine(result);    
    }
}
