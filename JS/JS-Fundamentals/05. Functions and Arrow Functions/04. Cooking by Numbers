function solve(array) {

    let number = Number(array[0]);

    for (let i = 1; i < array.length; i++) {
        if(array[i] == "chop"){
            number/=2;
            console.log(number)
        }
        if(array[i] == "dice"){
            number = Math.sqrt(number);
            console.log(number)
        }
        if(array[i] == "spice"){
            number+=1;
            console.log(number)
        }
        if(array[i] == "bake"){
            number*=3;
            console.log(number)
        }
        if(array[i] == "fillet"){
            number = number - (number * 0.2);
            console.log(number)
        }
    }
}
