function solve(array) {

    console.log(array.sort(sorting).join('\n'));

    function sorting(a,b) {
        if (a.length < b.length){
            return -1;
        }else if(a.length > b.length){
            return 1;
        }else {
            if(a < b){
                return -1;
            }else if(a > b){
                return 1;
            }else {
                return 0;
            }
        }
    }
}
